{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "jsx": "react",
    "strict": true,
    "baseUrl": ".",
    "paths": {
      "@components/*": ["src/components/*"],
      "@constants/*": ["src/navigation/constants/*"],
      "@assets/*": ["assets/*"],
      "@screens/*": ["src/screens/*"],
      "@navigation/*": ["src/navigation/*"],
      "@themes/*": ["src/theme/*"],
      "@store/*": ["src/store/*"],
      "@api/*": ["src/api/*"],
      "@env": ["./node_modules/react-native-dotenv"],
      "@hook/*": ["src/hook/*"],
    }
  }
}
